{
  "name": "@dicedb/console-electron",
  "productName": "console-electron",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "PORT=4000 node out/standalone/server.js",
    "start:forge": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "dev": "concurrently --kill-others \"next dev -p ${PORT-4000}\" \"electron .\"",
    "dev:next": "next dev -p ${PORT-4000}",
    "build": "next build && cp -R ./out/static ./out/standalone/out/static",
    "lint": "eslint ."
  },
  "dependencies": {
    "@next/env": "^14.2.15",
    "electron-log": "^5.2.0",
    "fix-path": "^4.0.0",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@dicedb/eslint-config": "*",
    "@dicedb/typescript-config": "*",
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/core": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.0.1",
    "electron": "^32.2.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
